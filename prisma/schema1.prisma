generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_dynamics")
}

model Rates {
  EXGTBLID   String
  CURNCYID   String
  EXCHDATE   DateTime
  TIME1      DateTime
  XCHGRATE   Float
  EXPNDATE   DateTime
  DEX_ROW_ID Int      

  //proforma Proforma @relation(fields: [SOPNUMBE, SOPTYPE], references: [SOPNUMBE, SOPTYPE], onDelete: NoAction, onUpdate: NoAction)
  @@map("MC00100")
  @@id([EXGTBLID, EXCHDATE, TIME1], map: "PKMC00100", clustered: false)
}